        <div class="span8 middle">

          <form class="form-horizontal">
            <fieldset>
              <h2 class="modal-header">Create New Group</h2>
              <div class="control-group">
                <label class="control-label" for="input01">Group Name</label>
                <div class="controls">
                  <input type="text" class="input-xlarge" id="input01"></div>
              </div>
              <div class="control-group">
                <label class="control-label" for="input01">Pick a Category</label>
                <div class="controls">

                  <select class="span3">
                    <option>1</option>

                    <option>2</option>
                    <option>3</option>
                    <option>4</option>
                    <option>5</option>
                  </select>
                </div>
              </div>
              <div class="control-group">

                <label class="control-label" for="input01">Descritpion</label>
                <div class="controls">
                  <textarea class="info" style="margin-top: 0px; margin-bottom: 0px; height: 140px; margin-left: 0px; margin-right: 0px; width: 271px; "></textarea>
                </div>
              </div>
              <div class="control-group">
                <label class="control-label">Privacy</label>

                <div class="controls">
                  <label class="radio">
                    <input type="radio" name="optionsRadios" id="optionsRadios1" value="option1" checked>Public - Any one can see and post on the group</label>
                  <label class="radio">
                    <input type="radio" name="optionsRadios" id="optionsRadios2" value="option2">Closed - Members only can see and post on the group</label>
                </div>

              </div>

              <div class="control-group">
                <label class="control-label" for="input01">Administrators</label>
                <div class="controls">
                  <input type="text" class="input-xlarge" id="input01"></div>
              </div>

              <div class="control-group">

                <label class="control-label" for="input01">Invite Friends</label>
                <div class="controls">
                  <input type="text" class="input-xlarge" id="input01"></div>
              </div>
              <div class="form-actions">
                <button type="submit" class="btn btn-primary">Create Group</button>
                <button class="btn">Cancel</button>
              </div>

            </fieldset>
